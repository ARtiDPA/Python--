import random
aboba_chet_chervi = [2,3,4,6,7,8,9,10,11]
aboba_chet_picki = [2,3,4,6,7,8,9,10,11]
aboba_chet_kresti = [2,3,4,6,7,8,9,10,11]
aboba_chet_bubi = [2,3,4,6,7,8,9,10,11]
dengi = 1000
def nachalo():
    print("Ставка: ")
    stavka = int(input())
    if stavka>dengi:
        nachalo()
    elif stavka<dengi:
        igra()

def igra():
    b = random.randint(0,4)
    c = random.randint(-1,8)
    print(b,c)
    
nachalo()    
    
